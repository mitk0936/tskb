{
  "name": "taskflow-docs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "tskb documentation for TaskFlow example application",
  "scripts": {
    "clean": "rimraf dist node_modules",
    "build": "node ../../packages/tskb/dist/cli/index.js \"**/*.tskb.tsx\" --out ./dist/taskflow-graph.json --tsconfig tsconfig.json",
    "visualize": "node ../../packages/tskb/dist/cli/index.js visualize taskflow-graph.json --out ./dist/architecture.dot",
    "generate": "mkdirp dist && npm run build && npm run visualize"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tskb": "*"
  }
}
