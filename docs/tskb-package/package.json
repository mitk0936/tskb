{
  "name": "tskb-meta-docs",
  "version": "1.0.0",
  "description": "<tskb> docs for the library package itself",
  "type": "module",
  "scripts": {
    "build": "node ../../packages/tskb/dist/cli/index.js \"**/*.tskb.tsx\" --out ./dist/taskflow-graph.json --tsconfig tsconfig.json",
    "visualize": "node ../../packages/tskb/dist/cli/index.js visualize ./dist/taskflow-graph.json --out ./dist/architecture.dot",
    "generate": "mkdirp dist && npm run build && npm run visualize",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "tskb": "*"
  },
  "author": "Dimitar Mihaylov",
  "license": "ISC"
}
